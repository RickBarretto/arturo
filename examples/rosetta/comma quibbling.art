quibble: $[sequence :block][

    len: size sequence

    if zero? len -> return "{}"
    if one? len  -> return ~"{|sequence\0|}"

    last: pop 'sequence
    ~Â« {|join.with: ", " sequence| and |last|}
    
]

sentences: [
	[] 
	["ABC"] 
	["ABC" "DEF"] 
	["ABC" "DEF" "G" "H"]
]

loop sentences 'sentence [
	print quibble sentence
]